{
  "name": "react-speed-analyzer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "start:modules": "vite -c vite.config.modules.js",
    "start:modules:analyzer": "cross-env VITE_REACT_APP_SCREEN_TYPE=analyzer vite -c vite.config.modules.js",
    "start:modules:result": "cross-env VITE_REACT_APP_SCREEN_TYPE=result vite -c vite.config.modules.js",
    "start:modules:test": "cross-env VITE_REACT_APP_SCREEN_TYPE=test vite -c vite.config.modules.js",
    "start:app:staging": "vite --mode staging",
    "start:app:prod": "vite preview",
    "start:modules:staging": "vite --mode staging -c vite.config.modules.js",
    "start:modules:prod": "vite preview -c vite.config.modules.js",
    "build:staging": "npm run build:app:staging && npm run build:modules:staging",
    "build:app:staging": "vite build --mode staging",
    "build:modules:staging": "vite build --mode staging -c vite.config.modules.js",
    "build": "npm run build:prod",
    "build:prod": "npm run build:app:prod && npm run build:modules:prod",
    "build:app:prod": "vite build",
    "build:modules:prod": "vite build -c vite.config.modules.js",
    "deploy:dev": "npm run deploy:app:dev && npm run deploy-code:dev",
    "deploy:app:dev": "npm run build:app:dev && baqend deploy -F --file-dir build --bucket-path www makefast-dev",
    "deploy:modules:dev": "npm run build:modules:dev && baqend deploy -F --file-dir build-modules --bucket-path embedded makefast-dev",
    "deploy:staging": "npm run deploy:app:staging && npm run deploy-code:staging",
    "deploy:app:staging": "npm run build:app:staging && baqend deploy -F --file-dir build --bucket-path www makefast-staging",
    "deploy:modules:staging": "npm run build:modules:staging && baqend deploy -F --file-dir build-modules --bucket-path embedded makefast-staging",
    "deploy:prod": "npm run deploy:app:prod && npm run deploy-code:prod",
    "deploy:app:prod": "npm run build:app:prod && baqend deploy -F --file-dir build --bucket-path www makefast",
    "deploy:modules:prod": "npm run build:modules:prod && baqend deploy -F --file-dir build-modules --bucket-path embedded makefast",
    "deploy-code:dev": "node scripts/code.js --mode=development",
    "deploy-code:staging": "node scripts/code.js --mode=staging",
    "deploy-code:prod": "node scripts/code.js --mode=production",
    "schema-download:dev": "baqend schema download makefast-dev",
    "schema-upload:dev": "baqend schema upload makefast-dev",
    "schema-upload:staging": "baqend schema upload makefast-staging",
    "schema-upload:prod": "baqend schema upload makefast",
    "build-proxy": "cd makefast-proxy && node build.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@types/express": "^4.17.12",
    "@uiw/react-codemirror": "^3.0.12",
    "baqend": "^3.1.1",
    "chalk": "4.1.1",
    "html-react-parser": "^1.2.7",
    "husky": "^7.0.0",
    "is-obj": "^3.0.0",
    "is-regexp": "^3.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2",
    "punycode": "^2.1.1",
    "query-string": "^7.0.1",
    "raf": "3.4.1",
    "react": "^17.0.2",
    "react-circular-progressbar": "^2.0.4",
    "react-css-collapse": "^4.1.0",
    "react-dev-utils": "11.0.4",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.3",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.28.1",
    "react-toggle": "^4.1.2",
    "react-tooltip": "^4.2.21",
    "redux": "^4.1.0",
    "redux-baqend": "^1.1.1",
    "redux-logger": "^3.0.6",
    "tachyons": "^4.12.0",
    "typeface-open-sans": "^1.1.13",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.10",
    "@vitejs/plugin-react-refresh": "^1.3.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.6",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "postcss": "^6.0.23",
    "postcss-calc": "^8.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^5.0.5",
    "rollup-plugin-copy": "^3.4.0",
    "vite": "^2.4.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
