{
  "name": "react-speed-analyzer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env REACT_APP_BAQEND=makefast node scripts/start.js",
    "start:modules": "cross-env REACT_APP_BAQEND=makefast node scripts/start-modules.js",
    "start:modules:analyzer": "cross-env REACT_APP_BAQEND=makefast REACT_APP_SCREEN_TYPE=analyzer node scripts/start-modules.js",
    "start:modules:result": "cross-env REACT_APP_BAQEND=makefast REACT_APP_SCREEN_TYPE=result node scripts/start-modules.js",
    "start:modules:test": "cross-env REACT_APP_BAQEND=makefast REACT_APP_SCREEN_TYPE=test node scripts/start-modules.js",
    "start:app:dev": "cross-env REACT_APP_BAQEND=makefast-dev node scripts/start.js",
    "start:app:staging": "cross-env REACT_APP_BAQEND=makefast-staging node scripts/start.js",
    "start:app:prod": "cross-env REACT_APP_BAQEND=makefast node scripts/start.js",
    "start:modules:dev": "cross-env REACT_APP_BAQEND=makefast-dev node scripts/start-modules.js",
    "start:modules:staging": "cross-env REACT_APP_BAQEND=makefast-staging node scripts/start-modules.js",
    "start:modules:prod": "cross-env REACT_APP_BAQEND=makefast node scripts/start-modules.js",
    "build:dev": "npm run build:app:dev && npm run build:modules:dev",
    "build:app:dev": "cross-env REACT_APP_BAQEND=makefast-dev node scripts/build.js",
    "build:modules:dev": "cross-env REACT_APP_BAQEND=makefast-dev node scripts/build-modules.js",
    "build:staging": "npm run build:app:staging && npm run build:modules:staging",
    "build:app:staging": "cross-env REACT_APP_BAQEND=makefast-staging node scripts/build.js",
    "build:modules:staging": "cross-env REACT_APP_BAQEND=makefast-staging node scripts/build-modules.js",
    "build": "npm run build:prod",
    "build:prod": "npm run build:app:prod && npm run build:modules:prod",
    "build:app:prod": "cross-env REACT_APP_BAQEND=makefast node scripts/build.js",
    "build:modules:prod": "cross-env REACT_APP_BAQEND=makefast node scripts/build-modules.js",
    "deploy:dev": "npm run deploy:app:dev && npm run deploy-code:dev",
    "deploy:app:dev": "npm run build:app:dev && baqend deploy -F --file-dir build --bucket-path www makefast-dev",
    "deploy:modules:dev": "npm run build:modules:dev && baqend deploy -F --file-dir build-modules --bucket-path embedded makefast-dev",
    "deploy:staging": "npm run deploy:app:staging && npm run deploy-code:staging",
    "deploy:app:staging": "npm run build:app:staging && baqend deploy -F --file-dir build --bucket-path www makefast-staging",
    "deploy:modules:staging": "npm run build:modules:staging && baqend deploy -F --file-dir build-modules --bucket-path embedded makefast-staging",
    "deploy:prod": "npm run deploy:app:prod && npm run deploy-code:prod",
    "deploy:app:prod": "npm run build:app:prod && baqend deploy -F --file-dir build --bucket-path www makefast",
    "deploy:modules:prod": "npm run build:modules:prod && baqend deploy -F --file-dir build-modules --bucket-path embedded makefast",
    "deploy-code:dev": "node scripts/code.js --mode=development",
    "deploy-code:staging": "node scripts/code.js --mode=staging",
    "deploy-code:prod": "node scripts/code.js --mode=production",
    "schema-download:dev": "baqend schema download makefast-dev",
    "schema-upload:dev": "baqend schema upload makefast-dev",
    "schema-upload:staging": "baqend schema upload makefast-staging",
    "schema-upload:prod": "baqend schema upload makefast",
    "build-proxy": "cd makefast-proxy && node build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-jest": "22.0.0",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "6.26.0",
    "baqend": "^2.12.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "flow-bin": "^0.60.1",
    "fs-extra": "3.0.1",
    "html-react-parser": "^0.6.2",
    "html-webpack-plugin": "2.29.0",
    "husky": "^0.14.3",
    "jest": "20.0.4",
    "lint-staged": "^7.2.2",
    "lodash.merge": "^4.6.0",
    "object-assign": "4.1.1",
    "object-hash": "^1.2.0",
    "postcss-calc": "^6.0.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "2.1.0",
    "postcss-nested": "3.0.0",
    "postcss-wrap": "^0.0.4",
    "prettier": "^1.14.2",
    "promise": "8.0.1",
    "promise-polyfill": "^7.0.0",
    "prop-types": "^15.6.2",
    "punycode": "^2.1.1",
    "query-string": "^5.0.1",
    "raf": "3.4.0",
    "react": "^16.5.2",
    "react-codemirror": "^1.0.0",
    "react-codemirror2": "^4.0.0",
    "react-css-collapse": "^3.5.0",
    "react-dev-utils": "^4.2.2",
    "react-dom": "^16.5.2",
    "react-modal": "^3.5.1",
    "react-redux": "^5.0.6",
    "react-render-html": "^0.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.23.0",
    "react-toggle": "^4.0.2",
    "react-tooltip": "^3.8.4",
    "react-transition-group": "^2.4.0",
    "redux": "^3.7.2",
    "redux-baqend": "^1.1.0",
    "redux-logger": "^3.0.6",
    "style-loader": "0.20.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "tachyons": "^4.11.1",
    "typeface-open-sans": "^0.0.54",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "^2.0.3",
    "yarn": "^1.10.0"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.1.2",
    "babel-eslint": "8.0.3",
    "baqend-coding-standard": "^1.1.2",
    "cross-env": "^5.2.0",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "^7.11.1",
    "get-own-enumerable-property-symbols": "^2.0.1",
    "is-obj": "^1.0.1",
    "is-regexp": "^1.0.0",
    "node-fetch": "^1.7.3",
    "rimraf": "^2.6.2",
    "typescript": "2.8.1",
    "which": "^1.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
